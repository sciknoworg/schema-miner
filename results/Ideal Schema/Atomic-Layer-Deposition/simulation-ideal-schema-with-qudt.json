          "properties": {
            "quantityValue": {
              "type": "object",
              "properties": {
                "numericValue": {
                  "type": "number"
                },
                "unit": {
                  "type": "object",
                  "properties": {
                    "hasQuantityKind": {
                      "type": "string",
