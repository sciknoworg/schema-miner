{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Atomic Layer Deposition (ALD) Process Schema",
  "type": "object",
  "properties": {
    "simulationParameters": {
      "type": "object",
      "description": "Parameters and methodologies used in the ALD simulation.",
      "properties": {
        "methods": {
          "type": "array",
          "description": "Computational methods used in the ALD process simulation.",
          "items": {
            "type": "string",
            "enum": [
              "DFT",
              "HF",
              "post-HF",
              "ML",
              "kMC",
              "MD",
              "Hybrid ONIOM",
              "CCSD(T)",
              "MP2",
              "Continuum Fluid Dynamics (CFD)",
              "RMD",
              "Monte Carlo",
              "Lattice Boltzmann Method (LBM)",
              "Group Contribution Method (GCM)",
              "Computer-Aided Molecular Design (CAMD)"
            ]
          }
        },
        "methodDetails": {
          "type": "object",
          "description": "Details specific to the computational methods used.",
          "properties": {
            "timestep": {
              "type": "object",
              "description": "Timestep used in MD simulations.",
              "properties": {
                "quantityValue": {
                  "type": "object",
                  "properties": {
                    "numericValue": {
                      "type": "number"
                    },
                    "unit": {
                      "type": "object",
                      "properties": {
                        "hasQuantityKind": {
                          "type": "string",
                          "enum": [
                            "http://qudt.org/vocab/quantitykind/Time"
                          ]
                        },
                        "sameAs": {
                          "type": "string",
                          "enum": [
                            "http://qudt.org/vocab/unit/FemtoSEC"
                          ]
                        }
                      }
                    }
                  }
                },
                "quantityKind": {
                  "type": "string",
                  "enum": [
                    "http://qudt.org/vocab/quantitykind/Time"
                  ]
                }
              }
            },
            "functional": {
              "type": "string",
              "description": "Functional used in DFT calculations, e.g., B3LYP."
            },
            "basisSet": {
              "type": "string",
              "description": "Basis set used in quantum chemical calculations, e.g., 6-31G(d,p)."
            },
            "clusterModel": {
              "type": "string",
              "description": "Cluster model used (e.g., Si9H12, Si23H24)"
            }
          }
        },
        "source": {
          "type": "object",
          "properties": {
            "method": {
              "type": "string",
              "description": "Method used to obtain the data (e.g., simulation, experiment)"
            }
          }
        }
      }
    },
    "materials": {
      "type": "object",
      "description": "Materials involved in the ALD process.",
      "properties": {
        "precursors": {
          "type": "array",
          "description": "List of precursors used in the ALD process.",
          "items": {
            "type": "string"
          }
        },
        "coReactants": {
          "type": "array",
          "description": "List of co-reactants used in the ALD process.",
          "items": {
            "type": "string"
          }
        },
        "substrates": {
          "type": "array",
          "description": "List of substrates used in the ALD process.",
          "items": {
            "type": "string"
          }
        },
        "encapsulationMaterials": {
          "type": "array",
          "description": "Materials used for encapsulation in selective growth.",
          "items": {
            "type": "string"
          }
        },
        "ligandModification": {
          "type": "object",
          "description": "Details of any modifications to ligands, such as conversion from methoxy to hydroxyl.",
          "properties": {
            "originalLigand": {
              "type": "string",
              "description": "Original ligand before modification."
            },
            "modifiedLigand": {
              "type": "string",
              "description": "Ligand after modification."
            }
          }
        }
      }
    },
    "growthRate": {
      "type": "object",
      "description": "Information related to the growth rate of the film.",
      "properties": {
        "rate": {
          "type": "object",
          "description": "Growth rate of the film per cycle.",
          "properties": {
            "quantityValue": {
              "type": "object",
              "properties": {
                "numericValue": {
                  "type": "number"
                },
                "unit": {
                  "type": "object",
                  "properties": {
                    "hasQuantityKind": {
                      "type": "string",
                      "enum": [
                        "http://qudt.org/vocab/quantitykind/Length"
                      ]
                    },
                    "sameAs": {
                      "type": "string",
                      "enum": [
                        "http://qudt.org/vocab/unit/ANGSTROM"
                      ]
                    }
                  }
                }
              }
            },
            "quantityKind": {
              "type": "string",
              "enum": [
                "http://qudt.org/vocab/quantitykind/Length"
              ]
            }
          }
        },
        "temperatureDependence": {
          "type": "object",
          "description": "Temperature dependence of the growth rate.",
          "properties": {
            "quantityValue": {
              "type": "object",
              "properties": {
                "numericValue": {
                  "type": "number"
                },
                "unit": {
                  "type": "object",
                  "properties": {
                    "hasQuantityKind": {
                      "type": "string",
                      "enum": [
                        "http://qudt.org/vocab/quantitykind/Temperature"
                      ]
                    },
                    "sameAs": {
                      "type": "string",
                      "enum": [
                        "http://qudt.org/vocab/unit/DEG_C"
                      ]
                    }
                  }
                }
              }
            },
            "quantityKind": {
              "type": "string",
              "enum": [
                "http://qudt.org/vocab/quantitykind/Temperature"
              ]
            }
          }
        },
        "propertySource": {
          "type": "string",
          "description": "Source of the property (e.g., calculated, observed)"
        }
      }
    },
    "surfaceProperties": {
      "type": "object",
