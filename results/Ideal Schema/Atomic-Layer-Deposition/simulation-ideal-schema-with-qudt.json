                "properties": {
                  "quantityValue": {
                    "type": "object",
                    "properties": {
                      "numericValue": {
                        "type": "number"
                      },
                      "unit": {
                        "type": "object",
                        "properties": {
                          "hasQuantityKind": {
                            "type": "string",
                            "enum": [
                              "https://qudt.org/vocab/quantitykind/Energy"
                            ]
                          },
                          "sameAs": {
                            "type": "string",
                            "enum": [
                              "https://qudt.org/vocab/unit/EV"
                            ]
                          }
