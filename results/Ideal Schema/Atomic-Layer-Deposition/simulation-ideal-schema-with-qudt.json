                "numericValue": {
                  "type": "number"
                },
                "unit": {
                  "type": "object",
                  "properties": {
                    "hasQuantityKind": {
                      "type": "string",
                      "enum": [
                        "http://qudt.org/vocab/quantitykind/Frequency"
                      ]
                    },
                    "sameAs": {
                      "type": "string",
                      "enum": [
                        "http://qudt.org/vocab/unit/HZ"
                      ]
                    }
                  }
                }
              }
            },
            "quantityKind": {
              "type": "string",
              "enum": [
                "http://qudt.org/vocab/quantitykind/Frequency"
              ]
            }
          }
        },
        "diffusionRate": {
          "type": "object",
          "description": "Rate of surface diffusion.",
          "properties": {
            "quantityValue": {
              "type": "object",
              "properties": {
                "numericValue": {
                  "type": "number"
                },
                "unit": {
                  "type": "object",
                  "properties": {
                    "hasQuantityKind": {
                      "type": "string",
