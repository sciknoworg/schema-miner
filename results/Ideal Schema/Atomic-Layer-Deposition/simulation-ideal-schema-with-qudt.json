                "numericValue": {
                  "type": "number"
                },
                "unit": {
                  "type": "object",
                  "properties": {
                    "hasQuantityKind": {
                      "type": "string",
                      "enum": [
                        "https://qudt.org/vocab/quantitykind/ChemicalAffinity"
                      ]
                    },
                    "sameAs": {
                      "type": "string",
                      "enum": [
                        "https://qudt.org/vocab/unit/KiloJ-PER-MOL"
                      ]
                    }
                  }
                }
              }
            },
            "quantityKind": {
              "type": "string",
              "enum": [
                "https://qudt.org/vocab/quantitykind/ChemicalAffinity"
              ]
            }
          }
        },
        "surfaceCoverage": {
          "type": "object",
          "description": "Coverage of the surface by the adsorbate.",
          "properties": {
            "coverage": {
              "type": "object",
              "description": "Percentage of surface coverage.",
              "properties": {
                "quantityValue": {
                  "type": "object",
                  "properties": {
                    "numericValue": {
                      "type": "number"
