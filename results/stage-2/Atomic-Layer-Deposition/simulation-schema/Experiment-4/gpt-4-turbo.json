{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Atomic Layer Deposition Process",
    "type": "object",
    "properties": {
        "precursors": {
            "type": "object",
            "properties": {
                "TiI4": {
                    "type": "object",
                    "properties": {
                        "adsorptionDetails": {
                            "type": "object",
                            "properties": {
                                "adsorptionSites": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    },
                                    "description": "Preferred adsorption sites on the surface for TiI4."
                                },
                                "adsorptionEnergies": {
                                    "type": "object",
                                    "properties": {
                                        "SiO2": {
                                            "type": "number",
                                            "description": "Adsorption energies on SiO2 surfaces for TiI4."
                                        }
                                    }
                                },
                                "reactionPathways": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    },
                                    "description": "Possible reaction pathways for TiI4 on SiO2."
                                }
                            }
                        }
                    }
                },
                "H2O": {
                    "type": "object",
                    "properties": {
                        "reactivity": {
                            "type": "string"
                        },
                        "reactionMechanism": {
                            "type": "string",
                            "description": "Mechanism of H2O interaction with precursors."
                        },
                        "products": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "surfaceSites": {
            "type": "object",
            "properties": {
                "isolatedOH": {
                    "type": "object",
                    "properties": {
                        "reactivity": {
                            "type": "string",
                            "description": "Reactivity of isolated -OH groups on SiO2 surface."
                        }
                    }
                },
                "adjacentHydrogenBondedOH": {
                    "type": "object",
                    "properties": {
                        "reactivity": {
                            "type": "string",
                            "description": "Reactivity of adjacent hydrogen-bonded -OH groups on SiO2 surface."
                        }
                    }
                },
                "siloxaneBridges": {
                    "type": "object",
                    "properties": {
                        "reactivity": {
                            "type": "string",
                            "description": "Reactivity of siloxane bridges on SiO2 surface."
                        }
                    }
                }
            }
        },
        "processConditions": {
            "type": "object",
            "properties": {
                "temperature": {
                    "type": "number",
                    "description": "Optimal temperature range for the ALD process."
                },
                "pressure": {
                    "type": "number",
                    "description": "Optimal pressure for the ALD process."
                }
            }
        },
        "observableProperties": {
            "type": "object",
            "properties": {
                "filmProperties": {
                    "type": "object",
                    "properties": {
                        "electricalProperties": {
                            "type": "string"
                        },
                        "barrierEffectiveness": {
                            "type": "string"
                        },
                        "crystallineStructure": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}
